{
  "name": "full-stack-sample-app",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules",
    "build": "npm ci && npm run client-build",
    "start": "NODE_ENV=production node server/bin/www",
    "reinstall": "npm run clean && npm install",
    "dev": "npm-run-all --parallel server-dev client-dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server-dev": "nodemon server/bin/www",
    "client-dev": "react-app-rewired start",
    "client-build": "BUILD_PATH='./server/public/dist' react-app-rewired build",
    "client-test": "react-app-rewired test",
    "client-eject": "react-app-rewired eject"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-app-rewired": "^2.2.1"
  }
}
